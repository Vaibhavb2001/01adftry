{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vb-adf3011"
		},
		"LS_AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSqlDatabase1'"
		},
		"LS_AzureSqlDatabase2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSqlDatabase2'"
		},
		"LS_BLOB_0201_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BLOB_0201'"
		},
		"LS_DLGEN_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DLGEN'"
		},
		"LS_AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlvb1.database.windows.net"
		},
		"LS_AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "cloudsqldatab"
		},
		"LS_AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vb050125"
		},
		"LS_AzureSqlDatabase2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlvb0125.database.windows.net"
		},
		"LS_AzureSqlDatabase2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldatab1"
		},
		"LS_AzureSqlDatabase2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vb050125"
		},
		"LS_DLGEN_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage3011gen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vb-adf3011_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureSqlDatabase1_password": {
						"value": "[parameters('LS_AzureSqlDatabase1_password')]"
					},
					"LS_AzureSqlDatabase2_password": {
						"value": "[parameters('LS_AzureSqlDatabase2_password')]"
					},
					"LS_BLOB_0201_connectionString": {
						"value": "[parameters('LS_BLOB_0201_connectionString')]"
					},
					"LS_DLGEN_accountKey": {
						"value": "[parameters('LS_DLGEN_accountKey')]"
					},
					"LS_AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"LS_AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"LS_AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"LS_AzureSqlDatabase2_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSqlDatabase2_properties_typeProperties_server')]"
					},
					"LS_AzureSqlDatabase2_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSqlDatabase2_properties_typeProperties_database')]"
					},
					"LS_AzureSqlDatabase2_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSqlDatabase2_properties_typeProperties_userName')]"
					},
					"LS_DLGEN_properties_typeProperties_url": {
						"value": "[parameters('LS_DLGEN_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "vb-adf3011_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'vb-adf3011_ArmTemplate_0')]"
			]
		}
	]
}